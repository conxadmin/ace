  conx-apache-ace-copy-repo:
   environment:
     SOURCE_ORG_APACHE_ACE_SERVER: localhost:8080
     SOURCE_ORG_APACHE_ACE_OBR localhost:8080
     REPO_EXPORTER_TARGETS target1;target2
   image: conxadmin/apache-ace-conx-client-copy-repo
  conx-apache-ace-server-allinone:
   environment:
     DEFAULT_HOST: local.dev
